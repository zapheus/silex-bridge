{
    "name": "zapheus/silex-bridge",
    "description": "Silex Service Provider bridge for Zapheus framework.",
    "keywords": ["bridge", "service provider", "silex", "zapheus"],
    "homepage": "https://github.com/zapheus/silex-bridge",
    "license": "MIT",
    "authors": [
        {
            "name": "Rougin Royce Gutib",
            "email": "rougingutib@gmail.com",
            "homepage": "https://rougin.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "pimple/pimple": "~3.0",
        "zapheus/zapheus": "~0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2|~5.7",
        "scrutinizer/ocular": "~1.1.0"
    },
    "autoload": {
        "psr-4": {
            "Zapheus\\Bridge\\Silex\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zapheus\\Bridge\\Silex\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}